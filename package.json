  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/katex": "^0.16.7",
    "@types/markdown-it": "^13.0.7",
    "@types/markdown-it-container": "^2.0.10",
    "@types/markdown-it-emoji": "^2.0.4",
    "@types/markdown-it-footnote": "^3.0.3",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anuragkr29/md-preview-pdf.git"
