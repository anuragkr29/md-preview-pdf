{
  "name": "md-preview-pdf",
  "version": "1.0.0",
  "description": "State-of-the-art Markdown to PDF converter with Mermaid diagrams, syntax highlighting, math equations, and more",
  "main": "dist/index.js",
  "bin": {
    "md-preview-pdf": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "markdown",
    "pdf",
    "converter",
    "mermaid",
    "syntax-highlighting",
    "katex",
    "gfm",
    "github-flavored-markdown"
  ],
  "author": "Anurag Kumar <anuragkr29@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "katex": "^0.16.9",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-toc-done-right": "^4.2.0",
    "mermaid": "^10.6.1",
    "ora": "^5.4.1",
    "puppeteer": "^24.34.0"
